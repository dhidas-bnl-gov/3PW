record(calc, "$(SYS){$(DEV)}BeamPipeSwitchAny") {
  field(INPA, "$(SYS){$(DEV)}BeamPipeSwitch1")
  field(INPB, "$(SYS){$(DEV)}BeamPipeSwitch2")
  field(INPC, "$(SYS){$(DEV)}BeamPipeSwitch3")
  field(INPD, "$(SYS){$(DEV)}BeamPipeSwitch4")
  field(CALC, "A|B|C|D")
}


